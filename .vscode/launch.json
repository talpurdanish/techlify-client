  "version": "0.2.0",
  "configurations": [
  {
    "name": "Attach to firefox",
    "port": 4000,
    "request": "attach",
    "type": "firefox",
    "webRoot": "${workspaceFolder}",
    "url": "http://localhost:4000/"
  },
    {
      "name": "ng serve",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200/"
    },
    {
      "name": "ng test",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: test",
      "url": "http://localhost:9876/debug.html"
    }
  ]
}
